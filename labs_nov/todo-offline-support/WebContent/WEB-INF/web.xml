<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>todo-offline-support</display-name>
  <welcome-file-list>
    <welcome-file>todo.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>TodoServlet</display-name>
    <servlet-name>TodoServlet</servlet-name>
    <servlet-class>com.bt.offline.todo.servlet.TodoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TodoServlet</servlet-name>
    <url-pattern>/todos</url-pattern>
  </servlet-mapping>
</web-app>